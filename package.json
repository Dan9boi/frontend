{
  "name": "24ways-frontend",
  "version": "2016.0.0",
  "description": "Bits, the front-end component library for 24 ways",
  "keywords": [
    "styleguide",
    "patterns",
    "library"
  ],
  "homepage": "http://bits.24ways.org",
  "bugs": "https://github.com/24ways/frontend/issues",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Paul Robert Lloyd",
  "repository": {
    "type": "git",
    "url": "https://github.com/24ways/frontend.git"
  },
  "main": "fractal.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node_modules/.bin/gulp start",
    "build": "node_modules/.bin/gulp build",
    "dev": "node_modules/.bin/gulp dev",
    "publish": "node_modules/.bin/gulp publish",
    "test": "xo || stylelint src/**/*.css"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "property-no-unknown": [ true, {
        "ignoreProperties": ["font-range"]
      }]
    }
  },
  "xo": {
    "space": true,
    "envs": "browser"
  },
  "dependencies": {
    "@frctl/fractal": "^1.1.0",
    "@frctl/mandelbrot": "^1.2.0",
    "@frctl/nunjucks": "^1.0.0",
    "autoprefixer": "^7.1.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.0",
    "cssnano": "^4.0.0-rc.2",
    "del": "^3.0.0",
    "fontfaceobserver": "^2.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-gh-pages": "^0.5.0",
    "gulp-imagemin": "^4.0.0",
    "gulp-postcss": "^7.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-util": "^3.0.0",
    "markdown-it": "^8.0.0",
    "markdown-it-abbr": "^1.0.0",
    "markdown-it-footnote": "^3.0.0",
    "nunjucks-date": "^1.2.0",
    "postcss-apply": "^0.8.0",
    "postcss-assets": "^5.0.0",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-map": "^0.9.0",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nested": "^2.1.0",
    "postcss-responsive-type": "^1.0.0",
    "postcss-simple-vars": "^4.1.0",
    "prismjs": "^1.8.0",
    "rollup": "^0.51.0",
    "rollup-plugin-babel": "^3.0.0",
    "rollup-plugin-closure-compiler-js": "^1.0.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "devDependencies": {
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "xo": "^0.18.0"
  }
}
